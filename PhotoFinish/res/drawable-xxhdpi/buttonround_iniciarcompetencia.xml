<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
 <item android:state_pressed="true" >
     <shape android:shape="rectangle"  >
         <corners android:radius="8dip" />
         <!--  Color de Contorno
         <stroke android:width="1dip" android:color="@color/rojoprofundo" />
         -->
         <gradient android:angle="-90" android:startColor="@color/red_nopressed" android:endColor="@color/red_pressed"  />            
     </shape>
 </item>
